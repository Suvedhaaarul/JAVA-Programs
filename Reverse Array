import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr1 = new int[n];
		for(int i=0;i<n;i++){
		    arr1[i]=sc.nextInt();
		}
		for(int i=n-1;i>=0;i--){
		    System.out.print(arr1[i]); 
		}
	}
}
